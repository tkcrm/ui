{
  "name": "@tkcrm/ui",
  "version": "0.0.1-alpha.5",
  "description": "TKCRM UI React components",
  "scripts": {
    "quick-build": "tsc",
    "quick-build-tests": "tsc -p test",
    "copy-root-files": "shx cp ../../README.md . && shx cp ../../LICENSE .",
    "build": "npm run quick-build && npm run copy-root-files && shx rm -rf dist && vite build && shx cp ./dist/tkcrm-ui.es.js ./dist/tkcrm-ui.es.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkcrm/ui.git"
  },
  "author": "sxwebdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tkcrm/ui/issues"
  },
  "homepage": "https://github.com/tkcrm/ui#readme",
  "source": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "webpack": "./dist/tkcrm-ui.es.js",
        "default": "./dist/tkcrm-ui.es.mjs"
      },
      "require": "./dist/tkcrm-ui.umd.js",
      "script": "./dist/tkcrm-ui.umd.js",
      "default": "./dist/tkcrm-ui.es.mjs"
    }
  },
  "esmodule": "./dist/tkcrm-ui.es.mjs",
  "module": "./dist/tkcrm-ui.es.js",
  "jsnext:main": "./dist/tkcrm-ui.es.js",
  "react-native": "./dist/tkcrm-ui.es.js",
  "umd:main": "./dist/tkcrm-ui.umd.js",
  "unpkg": "./dist/tkcrm-ui.umd.js",
  "jsdelivr": "./dist/tkcrm-ui.umd.js",
  "main": "./dist/tkcrm-ui.umd.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "react": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "mobx": "^6.0.0",
    "mobx-keystone": "^0.67.1",
    "mobx-react": "^7.3.0"
  },
  "devDependencies": {
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@babel/core": "^7.17.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@tailwindcss/forms": "^0.5.0",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.21",
    "autoprefixer": "^10.4.4",
    "mobx": "^6.4.2",
    "mobx-react": "^7.3.0",
    "mobx-keystone": "^0.67.1",
    "postcss": "^8.4.12",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.0",
    "fastest-validator": "^1.12.0",
    "lodash": "^4.17.21",
    "rc-field-form": "^1.24.0",
    "rc-notification": "^4.5.7"
  }
}
