{
  "name": "@tkcrm/ui",
  "version": "0.0.4",
  "description": "Tailwind React UI components",
  "title": "TKCRM UI",
  "keywords": [
    "tkcrm",
    "ui",
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "tailwind",
    "form",
    "mobx",
    "mobx-keystone"
  ],
  "scripts": {
    "quick-build": "tsc",
    "copy-root-files": "shx cp ../../README.md . && shx cp ../../LICENSE .",
    "build": "npm run quick-build && npm run copy-root-files && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkcrm/ui.git"
  },
  "author": "sxwebdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tkcrm/ui/issues"
  },
  "homepage": "https://github.com/tkcrm/ui#readme",
  "source": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    "./dist/style.css": "./dist/style.css",
    ".": {
      "import": {
        "webpack": "./dist/index.es.js",
        "default": "./dist/index.es.mjs"
      },
      "require": "./dist/index.cjs.js",
      "script": "./dist/index.cjs.js",
      "default": "./dist/index.es.mjs"
    }
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "@headlessui/react": ">=1.5",
    "@heroicons/react": ">=1.0.6",
    "classnames": "^2",
    "dayjs": ">=1.11",
    "fastest-validator": ">=1.12",
    "lodash": "^4",
    "mobx": "^6",
    "mobx-keystone": ">=0.67",
    "mobx-react-component": ">=2.7",
    "rc-field-form": ">=1.26.2",
    "rc-notification": ">=4.6",
    "react": "^17 || ^18",
    "react-router-dom": "^6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.0",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.24",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@types/tailwindcss": "^3.0.10",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "^10.4.4",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.1",
    "fastest-validator": "^1.12.0",
    "lodash": "^4.17.21",
    "mobx": "^6.5.0",
    "mobx-keystone": "^0.67.3",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "rc-field-form": "^1.26.2",
    "rc-notification": "^4.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vite": "^2.9.13"
  }
}
