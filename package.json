{
  "root": true,
  "private": true,
  "name": "root",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lib:build": "cd packages && cd lib && npm run build",
    "lib:publish": "cd packages/lib && npm publish --access public",
    "site:start": "npm start --prefix packages/site",
    "site:build": "cd packages && cd site && npm run build",
    "lint": "cd packages && cd lib && npm run lint"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^17.0.41",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.12.0",
    "prettier": "^2.6.0",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "typescript": "^4.6.2"
  }
}
